<div class="form-row">
<div class='nested-fields col-md-12'>
  <div class="field">
    <%# f.label :date %>
    <%= f.text_field :date, :data => {:provide => "datepicker"}, :class => "form-control" %>
  </div>
  <%= link_to_remove_association "Remove Date", f, :class => "btn btn-block btn-danger" %>
</div>
</div>